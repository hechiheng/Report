<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN"
"http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="home.Login">
	<typeAlias alias="member" type="com.report.manage.bean.Member"/>
	
	<select id="home.LoginDao.getMember" parameterClass="member" resultClass="member">
	<![CDATA[
		select id,accountid,factname from t_member where accountid=#accountid# and password=#password# 
	]]>
	</select>
	
</sqlMap>