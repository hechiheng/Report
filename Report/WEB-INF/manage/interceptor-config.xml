<?xml version="1.0" encoding="UTF-8" ?>
<interceptor-config>
	<interceptor name="login" type="com.report.manage.interceptor.LoginInterceptor"/>
	<interceptor name="news" type="com.report.manage.interceptor.NewsInterceptor"/>
	<interceptor name="message" type="com.report.manage.interceptor.MessageInterceptor"/>
	<interceptor name="announce" type="com.report.manage.interceptor.AnnounceInterceptor"/>
	<interceptor name="about" type="com.report.manage.interceptor.AboutInterceptor"/>
	<interceptor name="website" type="com.report.manage.interceptor.WebsiteInterceptor"/>
	<interceptor name="menu" type="com.report.manage.interceptor.MenuInterceptor"/>
	<interceptor name="link" type="com.report.manage.interceptor.LinkInterceptor"/>

	<action type="com.report.manage.action.LoginAction">
		<interceptor name="login"/>
	</action>
	<action type="com.report.manage.action.NewsAction">
		<interceptor name="news"/>
	</action>
	<action type="com.report.manage.action.MessageAction">
		<interceptor name="message"/>
	</action>
	<action type="com.report.manage.action.AnnounceAction">
		<interceptor name="announce"/>
	</action>
	<action type="com.report.manage.action.AboutAction">
		<interceptor name="about"/>
	</action>
	<action type="com.report.manage.action.WebsiteAction">
		<interceptor name="website"/>
	</action>
	<action type="com.report.manage.action.MenuAction">
		<interceptor name="menu"/>
	</action>
	<action type="com.report.manage.action.LinkAction">
		<interceptor name="link"/>
	</action>
	
</interceptor-config>